/* pages/onlineElection/main/main.js.wxss */

.button {
  background-color: transparent;
  opacity: 0.6;
  width:4.5vh;
  height:4.5vh;
  /* padding: 5px; */
  /* height: 1em; */
  /* line-height: 100%; */
  /* text-align: center; */
  position: absolute;
  top: 40%;
  left: 85%;
  z-index: 101;
	border-radius:2rem;
	box-shadow:0 0 10px #aaa;
	overflow: hidden;
}
.button image{
	width:4.5vh;
	height:4.5vh;
}

.mov {
  height: 75vh;
  width: 100%;
  background-color: rgba(red, green, blue, 0.2);
}

.mt-top-con{
	height: 10vh;
}
/* flow 流程菜单 ↓ */

.flow-menu{
	box-sizing: border-box;
	transition: left 0.4s ease-in;
	min-width: 50%;
	width: 50%;
	height: 100%;
	background-color: #2334a8;
	position: absolute;
	top: 0;
	left: 0;
	color: #fff;
	padding:2vh;
	font-size:2vh;
	z-index:10000;
}
.flow-menu .row{
	padding:1vh 0;
	position: relative;
}
/* 横线 */
/*  border-bottom: 1px solid #3E51D1;
	box-shadow: 0 1px 0px #0D1C7B; */
.flow-menu .row:nth-child(4), .flow-menu .row:nth-child(9){
	padding-bottom:3vh;
	margin-bottom: 2vh;
}
/* .flow-menu .row:nth-child(4) .line-blue-lignt, 
.flow-menu .row:nth-child(4) .line-blue, 
.flow-menu .row:nth-child(9) .line-blue-lignt,
.flow-menu .row:nth-child(9) .line-blue,
{
	height: 1px;
} */
.flow-menu-line{
	width:100%;
	height: 2px;
}
.flow-menu-line2{
	position: absolute;
	bottom:0;
	width:100%;
	height: 2px;
}
.flow-menu .flow-li{
	display:flex;
	align-items:center;
}
.flow-menu .flow-li-icon{
	width:2.4vh;
	height:2.4vh;
	display:inline-block;
	margin-right:1.8vh;
}
.flow-menu .flow-li-icon-right{
	width:2.4vh;
	height:2.4vh;
	display:inline-block;
	/* margin-right:2vh; */
}
.flow-menu .flow-li-text{
	display:inline-block;
}

.flow-title {
  font-size: 4vh;
  line-height: 15vh;
}

.meeting-body {
  transition: all 0.45s ease-in;
  position: relative;
  top: 0;
  left: 50%;

}
.meeting-hide {
  transition: all 0.4s ease-in;
  position: relative;
  top: 0;
  left: 0%;
}

.row {
  display: flex;
  justify-content: space-between;
}

/* flow 流程菜单 ↑ */

/** 内容展示区域 ↓ **/
.mt-con{
	width:100%;
	height:100%;
	position: absolute;
	top:0;
	left:0;
	z-index:0;
	box-sizing:border-box;
	padding:2vh;
}
.mt-con .mt-flow-btn-sign{
	position: absolute;
	top:20%;
	left: 50%;
}
.mt-con .mt-con-tip{
	/* text-align:center; */
	width:100%;
	display:block;
	line-height:10vh;
	color:#fff;
	font-size:3vh;
}
.mt-con .mt-con-sign button{
	width:60%;
	background-color:#67c23a;
	color:#fff;
}
/**************** 签到统计 ↓ ****************/
.mt-con .mt-con-sign-result-title{
	font-size: 3vh;
	line-height:15vh;
	color:#428BFA;
	text-align:center;
	font-weight:bold;
}
.mt-con-sign-result-con{
	display: flex;
	justify-content: space-around;
}
.mt-con-sign-result-wraper{
	color:#888;
	text-align:center;
	position: relative;
	width:14vh;
	height:14vh;
	display:flex;
	flex-direction: column;
	/* align-items: center; */
	justify-content:center;
}
.mt-con-sign-result-wraper image{
	position: absolute;
	z-index:-1;
	top:50%;
	left: 50%;
	width:14vh;
	height:14vh;
	transform:translate(-50%,-57%);
}
.mt-con-sign-result-wraper p{
	display: block;
	font-weight:bold;
	font-size:2vh;
}

.mt-con-sign-result-wraper p strong{
	font-size:3vh;
}
/**************** 签到统计 ↑ ****************/

/**************** 候选人介绍 投票 ↓ ****************/
.mt-flow-con-4, .mt-flow-con-8{
	width:100%;
	height:100%;
}
.mt-flow-con-tip{
	width:100%;
	display:block;
	font-size:3vh;
	margin:0.5vh 0;
	font-weight:bold;
}
.mt-flow-con-tip-vice{
	color:#A1A1A1;
}
.mt-flow-wraper{
	display: flex;
	justify-content: space-between;
	align-items: center;
	/* width:100%; */
	/* height:8vh; */
	height:auto;
	line-height:4vh;
	padding:2vh;
	border:1px solid #ddd;
	border-radius:1vh;
	/* box-shadow:0 0.3vh 0.5vh #aaa; */
	margin-bottom:1vh;
	color:#A1A1A1;
}
 .mt-flow-wraper-left, .mt-flow-wraper-right{
	disply:flex;
	justify-content: space-between;
	align-items:center;
	/* height: 8vh; */
	overflow: visible;
 }

.mt-flow-wraper image{
	width:2.5vh;
	height:2.5vh;
	margin:0 2vh 0 1vh;
}
.mt-flow-name, .mt-flow-wraper-right text{
	font-weight: bold;
	font-size:2.2vh;
	color:#000;
}
.mt-flow-age{
	margin-left:1vh;
}
.mt-flow-wraper-right text{
	color:#509EFF;
}
/* 投票 */
.mt-flow-con-8-wraper{
	border:1px solid #ddd;
	/* border-radius:1vh; */
	/* border-bottom-left-radius:0; */
	/* border-bottom-right-radius:0; */
	border-top-left-radius:0;
	border-top-right-radius:0;
	padding:0.5vh 2vh 0.5vh 0;
	margin-top:1vh;
}
.mt-flow-con-8 .mt-flow-wraper{
	border:none;
	border-bottom:1px dashed #ddd;
	padding:2vh 0;
}
.mt-flow-con-8 .mt-flow-wraper:last-child{
	border:none;
}
.mt-flow-con-8 .mt-flow-wraper image{
	margin:0 1vh;
}
.mt-flow-con-8 .mt-flow-wraper .mt-flow-age{
	margin:0;
}
.mt-flow-wraper-identity{
	display:inline-block;
	/* width:6vh; */
	min-width:2rem;
	padding:0 2vh 0 0.5vh;
	border-top-right-radius:3vh;
	border-bottom-right-radius:3vh;
	margin-right:2vh;
	color:#fff;

}
/* 我要投票按钮 */
.mt-flow-btn-border{
	color:#509EFF;
	border:1px solid #509EFF;
	border-bottom:none;
	border-radius:10vh;
	font-size:1.5vh;
	padding:0.5vh 2vh;
	margin-right:2vh;
}
/* 另选他人 */
.mt-flow-select-other-wraper{
	border:1px solid #ddd;
	/* border-radius:1vh; */
	border-top:none;
	border-bottom-left-radius:1vh;
	border-bottom-right-radius:1vh;
	/* padding:0.5vh 2vh 0.5vh 0; */
}
/* 另选他人 - 标题 */
.mt-flow-select-other-wraper-title{
	position:relative;
	height:2rem;
	line-height:2rem;
	color:#fff;
	font-size:0.8rem;
	font-weight:bold;
	text-align:center;
}
.mt-flow-select-other-wraper-title-line{
	width:100%;
	height:3px;
	background-color:#51A0FF;
}
.mt-flow-select-other-wraper-title-con{
	position:absolute;
	top:0;
	background-color:#51A0FF;
	height:100%;
	width:96%;
	left:2%;
	border-bottom-left-radius:1rem;
	border-bottom-right-radius:1rem;
	letter-spacing:1px;
}
/* 另选他人 - 书记 副书记 委员等等 */
.mt-flow-select-other-wraper-con{
	padding:2vh;
}
.mt-flow-select-other-wraper-con-name{
	width:15%;
	font-weight:bold;
	display:inline-block;
	min-width:2rem;
}
.mt-flow-select-other-row{
	line-height: 1.2rem;
	margin:0.5vh 0;
}
.mt-flow-select-other-wraper-con-name-con{
	width:83%;
	border-radius:4px;
	padding:0 1%;
	display: inline-block;
}
.mt-flow-select-other-wraper-title-icon{
	position:absolute;
	right:2vh;
	width:1rem;
	height:1rem;
	top:1.5vh;
}
.mt-flow-con-8-bottom{
	display: flex;
	justify-content: space-around;
	padding:2vh 2vh 4vh 2vh;
}
.mt-flow-con-8-bottom button{
	width:60%;
	font-size:0.8rem;
	border-radius:2rem;
	border:none;
}
/* 投票完成提示信息 */
.mt-flow-con-8-result-tip{
	line-height:10vh;
	font-size:2.6vh;
	color:#008000;
}
/**************** 候选人介绍 投票 ↑ ****************/

	
